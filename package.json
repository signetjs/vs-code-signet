{
    "name": "vs-code-signet",
    "displayName": "vs-code-signet",
    "description": "A signet helper plugin for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "cmstead",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other",
        "Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [],
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/vs-code-signet.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/vs-code-signet.json"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "quokka": {
        "plugins": [
            "quokka-mocha-bdd",
            "quokka-signet-explorer",
            "quokka-prerun"
        ],
        "quokka-mocha-bdd": {
            "interface": "bdd"
        },
        "quokka-prerun": {
            "prerunCommands": [
                "grunt build"
            ]
        }
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "approvals": "^2.1.2",
        "chai": "^4.0.2",
        "eslint": "^3.6.0",
        "mocha": "^2.3.3",
        "quokka-mocha-bdd": "^1.1.0",
        "quokka-prerun": "^1.0.0",
        "quokka-signet-explorer": "^1.1.1",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {}
}